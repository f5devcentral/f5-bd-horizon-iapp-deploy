      "value": "300"
    },		
    {
      "name": "pool__tcp_req_queuing",
      "encrypted": "no",
      "value": "no"
    },	
	{
      "name": "pool__use_pga",
      "encrypted": "no",
      "value": "no"
    },
	{
      "name": "pool__use_same_pool",
      "encrypted": "no",
	  "value": "yes"
    },
    {
      "name": "pool__use_slow_ramp",
      "encrypted": "no",
      "value": "yes"
    },
	{
      "name": "pool__vs_addr",
      "encrypted": "no",
	  "value": "{{iapp_vip_address}}"
    },
    {
      "name": "pool__vs_addr_internal",
      "encrypted": "no",
      "value": ""
    },		
    {
      "name": "pool__vs_port",
      "encrypted": "no",
      "value": "443"
    },	
	{
      "name": "pool__xff",
      "encrypted": "no",
      "value": "yes"
    },
	{
      "name": "server__tcp_opt",
      "encrypted": "no",
      "value": "/#lan#"
    },	
	{
      "name": "ssl__cert",
      "encrypted": "no",
	  "value": "{{iapp_ssl_cert}}"
    },
    {
      "name": "ssl__client_ssl_profile",
      "encrypted": "no",
      "value": "/#default#"
    },
	{
      "name": "ssl__key",
      "encrypted": "no",
	  "value": "{{iapp_ssl_key}}"
    },
    {
      "name": "ssl__mode",
      "encrypted": "no",
      "value": "client_ssl_server_ssl"
    },		
    {
      "name": "ssl__redirect_port",
      "encrypted": "no",
      "value": "80"
    },	
    {
      "name": "ssl__redirect_to_https",
      "encrypted": "no",
      "value": "yes"
    },	
	{
      "name": "ssl__server_ssl_profile",
      "encrypted": "no",
      "value": "/#default#"
    },
	{
      "name": "ssl__use_cert_chain",
      "encrypted": "no",
	  "value": "{{iapp_ssl_chain}}"
    },
    {
      "name": "stats__request_logging",
      "encrypted": "no",
      "value": "/#do_not_use#"
    }
  ]
}
